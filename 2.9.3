#include <iostream>
using namespace std;

int main(void){
    int vector[7]={1,3,5,8,7,3,1};
    int a=sizeof(vector)/sizeof(vector[0]);
    cout<<a<<endl;
    if(a%2==0){
        for(int i=0;i<a/2;i++){
            if(vector[i]!=vector[a-i-1]){
                cout<<"false";
                break;
            }
         
        }
    }
    else{
         for(int k=0;k<a/2;k++){
            if(vector[k]!=vector[a-k-1]){
                cout<<"false";
                break;
            }
            else{
                cout<<"true ";
            }
         }
    }
    
    
    return 0;
}
